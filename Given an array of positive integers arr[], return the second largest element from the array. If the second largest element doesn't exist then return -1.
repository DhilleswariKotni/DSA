class Solution {
  public:
    int getSecondLargest(vector<int> &arr) 
    {
        int large=INT_MIN;
        int sec_lar=-1;
        for(int i=0;i<arr.size();i++)
        {
            large=max(large,arr[i]);
        }
        for(int i=0;i<arr.size();i++)
        {
            if(arr[i]>sec_lar && arr[i]<large)
            {
                sec_lar=arr[i];
            }
        }
        return sec_lar;
    }
};
